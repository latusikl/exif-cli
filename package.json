{
  "name": "exif-cli",
  "version": "1.0.0",
  "description": "Command Line Interface tool for cleaning and viewing EXIF data.",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "start-watch": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' ./src/index.ts",
    "lint": "eslint . --ext .ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/*.spec.ts'"
  },
  "keywords": [
    "exif",
    "jpg",
    "clean",
    "view"
  ],
  "author": "≈Åukasz Latusik <latusikl@gmail.com",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "figlet": "^1.5.0",
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "eslint": "^7.20.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.0"
  }
}
